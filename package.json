{
  "name": "vsce-themes",
  "displayName": "Alex Gudkov Themes",
  "description": "Themes extension for VS Code",
  "keywords": [
    "theme",
    "icon-theme"
 ],
  "version": "0.5.1",
  "icon": "./icon.png",
  "engines": {
    "vscode": "*"
  },
  "author": "Alex Gudkov",
  "categories": [
    "Themes"
  ],
  "license": "MIT",
  "contributes": {
    "themes": [
      {
        "label": "Default Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/default-dark.json"
      },
      {
        "label": "Default Dark Plus",
        "uiTheme": "vs-dark",
        "path": "./themes/default-dark-plus.json"
      },
      {
        "label": "Alex Gudkov",
        "uiTheme": "vs-dark",
        "path": "./themes/alex-gudkov.json"
      },
      {
        "label": "Lutik",
        "uiTheme": "vs-dark",
        "path": "./themes/lutik.json"
      }
    ],
    "iconThemes": [
      {
        "id": "default-seti",
        "label": "Default Seti",
        "path": "./icon-themes/default-seti.json"
      },
      {
        "id": "alex-gudkov",
        "label": "Alex Gudkov",
        "path": "./icon-themes/alex-gudkov.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {},
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-gudkov/vsce-themes.git"
  },
  "publisher": "alex-gudkov"
