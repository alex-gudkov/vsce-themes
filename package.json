{
  "name": "vsce-themes",
  "displayName": "Themes",
  "description": "Themes extension for VS Code",
  "keywords": [
    "color",
    "theme",
    "alex",
    "gudkov"
  ],
  "version": "0.4.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "*"
  },
  "categories": [
    "Themes"
  ],
  "license": "MIT",
  "contributes": {
    "themes": [
      {
        "label": "Default Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/default-dark.json"
      },
      {
        "label": "Default Dark Plus",
        "uiTheme": "vs-dark",
        "path": "./themes/default-dark-plus.json"
      },
      {
        "label": "Alex Gudkov",
        "uiTheme": "vs-dark",
        "path": "./themes/alex-gudkov.json"
      },
      {
        "label": "Lutik",
        "uiTheme": "vs-dark",
        "path": "./themes/lutik.json"
      }
    ],
    "iconThemes": [
      {
        "id": "default-seti",
        "label": "Default Seti",
        "path": "./icon-themes/default-seti.json"
      },
      {
        "id": "alex-gudkov",
        "label": "Alex Gudkov",
        "path": "./icon-themes/alex-gudkov.json"
      }
    ],
    "snippets": [
      {
        "language": "",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "keybindings": [
      {
        "key": "alt+u",
        "command": "editor.action.transformToUppercase"
      },
      {
        "key": "alt+l",
        "command": "editor.action.transformToLowercase"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-gudkov/vsce-themes.git"
  },
  "publisher": "alex-gudkov"
}
