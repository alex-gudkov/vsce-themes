{
  "$schema": "vscode://schemas/color-theme",
  "name": "Alex Gudkov",
  "include": "./default-dark-plus.json",
  "colors": {
    "activityBar.background": "#010101",
    "editor.background": "#010101",
    "editorGroupHeader.tabsBackground": "#010101",
    "list.hoverBackground": "#464646",
    "sideBar.background": "#010101",
    "tab.inactiveBackground": "#010101",
    "titleBar.activeBackground": "#010101",
    "titleBar.inactiveBackground": "#403582",
    "editorCursor.background": "#010101",
    "terminalCursor.foreground": "#ffffff",
    "terminalCursor.background": "#010101",
    "editor.lineHighlightBorder": "#00000000",
    "titleBar.border": "#00000000",
    "statusBar.border": "#464646",
    "editorLineNumber.foreground": "#252525",
    "tab.border": "#00000000",
    "tab.activeBorder": "#00000000",
    "statusBar.background": "#010101",
    "editorCursor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#151515",
    "scrollbarSlider.background": "#151515",
    "list.inactiveSelectionBackground": "#151515",
    "tab.activeBackground": "#151515",
    "tab.hoverBackground": "#464646",
    "scrollbarSlider.activeBackground": "#464646",
    "scrollbarSlider.hoverBackground": "#464646",
    "editorWhitespace.foreground": "#252525",
    "sideBar.border": "#464646",
    "panel.border": "#464646",
    "editorGroupHeader.border": "#464646"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {}
}
